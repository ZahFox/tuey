{
  "name": "tuey",
  "version": "0.1.2",
  "description": "convenient devops tools",
  "main": "lib/index",
  "types": "lib/index",
  "bin": {
    "tuey": "./bin/tuey"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zahfox/tuey"
  },
  "scripts": {
    "dev": "tsc-watch --onSuccess \"node ./lib/index.js\"",
    "build": "tsc --p ./tsconfig.json",
    "check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "test": "jest --config jestconfig.json",
    "version": "npm run format && git add -A src",
    "tuey": "npm run build && tuey"
  },
  "keywords": [
    "tuey",
    "convenient",
    "devops",
    "tools",
    "typescript"
  ],
  "author": "William McCullough",
  "license": "MIT",
  "dependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.10",
    "commander": "^2.19.0",
    "fs-extra": "^7.0.1",
    "keypress": "^0.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.3.3",
    "winston": "^3.2.0"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.10",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^23.3.13",
    "@types/keypress": "^2.0.30",
    "@types/node": "^10.12.18",
    "@types/reflect-metadata": "^0.1.0",
    "@types/winston": "^2.4.4",
    "jest": "^24.0.0",
    "prettier": "^1.16.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "tsc-watch": "^1.1.34",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.4"
  }
}
